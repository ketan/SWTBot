<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.eclipse.swtbot.eclipse.test.junit4"
      label="SWTBot JUnit 4.x Headless launchers for Eclipse (incubation)"
      version="2.0.3.qualifier"
      provider-name="Eclipse.org">

   <description url="http://eclipse.org/">
      SWTBot Eclipse Headless Launchers to launch tests from within ant (incubation)
   </description>

   <copyright>
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <requires>
      <import plugin="org.eclipse.core.runtime.compatibility"/>
      <import plugin="org.junit4"/>
      <import plugin="org.apache.ant"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.equinox.app"/>
      <import plugin="org.eclipse.ui"/>
   </requires>

   <plugin
         id="org.eclipse.swtbot.ant.optional.junit4"
         download-size="0"
         install-size="0"
         version="0.0.0"
         fragment="true"
         unpack="false"/>

   <plugin
         id="org.eclipse.swtbot.eclipse.junit4.headless"
         download-size="0"
         install-size="0"
         version="0.0.0"/>

</feature>
